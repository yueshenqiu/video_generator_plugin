{
  "manifest_version": 1,
  "name": "视频生成插件",
  "version": "1.0.0",
  "description": "支持多服务商的 AI 视频生成插件，提供文生视频、图生视频、首尾帧控制等功能，支持火山引擎、阿里云、智谱等服务商",
  "author": {
    "name": "三花",
    "url": "https://github.com/yueshenqiu"
  },
  "license": "GPL-3.0",
  "host_application": {
    "min_version": "0.12..0"
  },
  "homepage_url": "https://github.com/yueshenqiu/video_generator_plugin",
  "repository_url": "https://github.com/yueshenqiu/video_generator_plugin",
  "keywords": [
    "video",
    "generation",
    "text-to-video",
    "image-to-video",
    "AI",
    "视频生成",
    "文生视频",
    "图生视频",
    "豆包",
    "通义万相",
    "CogVideoX"
  ],
  "categories": [
    "Content Generation",
    "Multimedia"
  ],
  "default_locale": "zh-CN",
  "plugin_info": {
    "is_built_in": false,
    "plugin_type": "general",
    "components": [
      {
        "type": "action",
        "name": "video_generate",
        "description": "智能视频生成 Action，根据用户描述自动生成视频"
      },
      {
        "type": "command",
        "name": "video_generator",
        "description": "视频生成命令，提供完整的视频生成控制功能"
      }
    ]
  }
}